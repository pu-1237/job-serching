<%= form_with model: user, url: account_path, local: true do |f| %>
    <%= f.label :email %>
    <%= f.text_field :email, class: 'form-control' %>
    <%= f.label :password %>
    <%= f.password_field :password, class: 'form-control', id: 'session_password' %>
    <%= f.submit nil, class: 'btn btn-primary'%>
<% end %>

