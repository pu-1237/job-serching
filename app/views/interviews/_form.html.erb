<%= form_with model: interview, local: true do |f| %>
    <%= f.number_field :limit, class: 'form-control'%>
    <%= f.label "開始時刻", {class:'control-label'}%><br>
    <%= f.datetime_select :start, {default: Date.today + 0.hours + 00.minutes, minute_step: 30}, class: 'form-control bootstrap-datetime-width' %><br>
    <%= f.submit nil, class: 'btn btn-primary js-event-create-btn' %>
<% end %>  